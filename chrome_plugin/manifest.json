{
  "name": "Vkontakte music",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://vk.com/audio*"],
      "css": ["mystyle.css"],
      "js": ["messanger.js","vk_api.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://vk.com/audio*"
  ]
}